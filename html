<script>
    function redirecionar() {
        var link = "https://videos-simpless.fun/esp2";
        
        // Pega os parâmetros da URL atual
        var currentSearchParams = new URLSearchParams(window.location.search);

        // Pega os parâmetros do link, se houver algum
        var linkSearchParams = new URLSearchParams(link.split('?')[1] || '');

        // Adiciona os parâmetros da URL atual que não estão presentes no link
        currentSearchParams.forEach(function (value, key) {
            if (!linkSearchParams.has(key)) {
                linkSearchParams.append(key, value);
            }
        });

        // Atualiza o link com os novos parâmetros
        link += '?' + linkSearchParams.toString();

        // Redireciona para o link atualizado
        window.location.href = link;
    }
</script>

